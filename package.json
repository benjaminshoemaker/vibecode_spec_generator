    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/postgres": "^0.10.0",
    "ai": "^5.0.93",
    "autoprefixer": "^10.4.22",
    "jszip": "^3.10.1",
    "lucide-react": "^0.554.0",
    "next": "^16.0.3",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "resend": "^6.5.2",
